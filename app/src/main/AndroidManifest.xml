<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ikona"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ikona"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".MyApp">

        <activity android:name=".Menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Compass"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name=".Compass"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".SaveCoordinates"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name=".SaveCoordinates"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".SavePosition"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name=".SavePosition"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

    </application>


</manifest>